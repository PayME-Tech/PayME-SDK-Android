<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingButton">
        <attr name="pbText" format="string"/>
        <attr name="pbLoadingText" format="string"/>
        <attr name="pbTextSize" format="dimension"/>
        <attr name="pbTextColor" format="color|reference"/>
        <attr name="pbProgressColor" format="color|reference"/>
    </declare-styleable>
    <declare-styleable name="CameraKitView">
        <attr name="android_adjustViewBounds" format="boolean" />
        <attr name="camera_aspectRatio" format="float" />
        <attr name="camera_facing" format="integer" />
        <attr name="camera_flash" format="integer" />
        <attr name="camera_focus" format="integer" />
        <attr name="camera_zoomFactor" format="float" />
        <attr name="camera_permissions" format="integer" />
        <attr name="camera_imageMegaPixels" format="float" />
        <attr name="camera_imageJpegQuality" format="integer" />
    </declare-styleable>

</resources>